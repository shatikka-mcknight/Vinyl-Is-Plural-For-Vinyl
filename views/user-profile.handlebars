<div class="pageContainer">
  <!--Website Description Begin-->
  <div class="float container2 justify-content-center card">
      <h2>Welcome {{user_name}}!</h2>
  
      <form class="form">
        <div class="dataForm">
          Username: <!--will remove
          <input class="form-input" type="text" id="name-signup" placeholder="Username" />-->
        </div>
        <div class="dataForm">
         Email:<!--will remove
          <input class="form-input" type="text" id="email-signup" placeholder="Email"/>-->
        </div>
        <div class="dataForm">
         <!--will remove
          <input class="form-input" type="password" id="password-signup" placeholder="Password" />-->
        </div>
        <div class="dataForm">
         <!--will remove
          <input class="form-input" type="password" id="confirm-password" placeholder="Confirm Password" />-->
        </div>

        <div class="dataForm">
          <button class="btn btn-primary" type="submit">Submit</button>
        </div>
        <div class="dataForm">
          <button class="btn btn-primary" id="updateUser" type="submit">Cancel</button>
        </div>
      </form>
    </div>
  <!--Website Description Ends-->

<!--User login Begin-->
<div class="container-fluid">
  <div class="container3 justify-content-center card">
    <!-- buy -->
 {{#if objs}}
     <table class="table">
         <thead>
         <tr>
             <th scope="col">Album Title</th>
             <th scope="col">Catalog</th>
             <th scope="col">Year</th>
             <!--<th scope="col">Condition</th>
             <th scope="col">Price</th>-->
         </tr>
         </thead>
         <tbody>
         {{#each objs}}
             <tr>
                 <td>{{this.title}}</td>
                 <td>{{this.Catalog}}</td>
                 <td>{{this.Year}}</td>
                 <!--<td>{{this.Condition}}</td>
                 <td>{{this.price}}</td>-->
             </tr>

         {{/each}}
         </tbody>
     </table>

 {{else}}
     <div class="row">
         <div class="col-md-12">
                 <h3>
                     You have no saved buying/favorites.
                 </h3>
         </div>
     </div>
 {{/if}}
  </div>
  <div class="container3 justify-content-center card">
      {{#if objs}}
 <table class="table">
     <thead>
     <tr>
         <th scope="col">Album Title</th>
         <th scope="col">Catalog</th>
         <th scope="col">Year</th>
         <!--<th scope="col">Condition</th>
         <th scope="col">Price</th>-->
     </tr>
     </thead>
     <tbody>
     {{#each objs}}
         <tr>
             <td>{{this.title}}</td>
             <td>{{this.Catalog}}</td>
             <td>{{this.Year}}</td>
             <!--<td>{{this.Condition}}</td>
             <td>{{this.price}}</td>-->
         </tr>

     {{/each}}
     </tbody>
 </table>

 {{else}}
     <div class="row">
         <div class="col-md-12">
                 <h3>
                     You haven't listed any records for sale.
                 </h3>
         </div>
     </div>

 {{/if}}
  </div>
  <!--User login Ends-->
</div>

<script src="./js/login.js"></script>